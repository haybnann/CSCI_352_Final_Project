        xmlns:local="clr-namespace:Ed"
        mc:Ignorable="d"
        Title="AddRecordWindow" Height="450" Width="800">
    <Grid Margin="108,10,206.333,7.667">
        <Label Content="Yearly Income:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="75,14,0,0" Width="125" Height="24"/>
